<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/relativelayout_row_schedule">
    <TextView
        android:text="@string/starttime_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_starttime_row_lesson_schedule"
        android:textAppearance="@style/ScheduleStartTime"
        android:fontFamily="@font/roboto_light"
        android:layout_marginTop="@dimen/margintop_card_row_lesson_schedule_outer"
        android:layout_marginLeft="@dimen/marginlr_card_row_lesson_schedule_outer" />
    <TextView
        android:text="@string/endtime_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_endtime_row_lesson_schedule"
        android:textAppearance="@style/ScheduleEndTime"
        android:fontFamily="@font/roboto_light"
        android:layout_alignLeft="@+id/textview_card_starttime_row_lesson_schedule"
        android:layout_alignRight="@+id/textview_card_starttime_row_lesson_schedule"
        android:layout_below="@+id/textview_card_starttime_row_lesson_schedule"
        android:layout_marginTop="@dimen/margintop_card_row_lesson_schedule_inner" />
    <TextView
        android:text="@string/subject_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_subject_row_lesson_schedule"
        android:textAppearance="@style/ScheduleSubject"
        android:fontFamily="@font/roboto_light"
        android:layout_alignBaseline="@+id/textview_card_starttime_row_lesson_schedule"
        android:layout_toRightOf="@+id/textview_card_starttime_row_lesson_schedule"
        android:layout_toLeftOf="@+id/textview_card_type_row_lesson_schedule"
        android:layout_marginLeft="@dimen/marginlr_card_row_lesson_schedule_outer"
        android:layout_marginRight="@dimen/marginlr_card_row_lesson_schedule_outer" />
    <TextView
        android:text="@string/building_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_buildingnumber_row_lesson_schedule"
        android:textAppearance="@style/ScheduleBuilding"
        android:fontFamily="@font/roboto_light"
        android:layout_alignLeft="@+id/textview_card_subject_row_lesson_schedule"
        android:layout_below="@+id/textview_card_subject_row_lesson_schedule"
        android:layout_marginTop="@dimen/margintop_card_row_lesson_schedule_inner" />
    <TextView
        android:text="@string/room_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_room_row_lesson_schedule"
        android:textAppearance="@style/ScheduleRoom"
        android:fontFamily="@font/roboto_light"
        android:layout_toRightOf="@+id/textview_card_buildingnumber_row_lesson_schedule"
        android:layout_alignBaseline="@+id/textview_card_buildingnumber_row_lesson_schedule" />
    <TextView
        android:text="@string/teacher_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_teacher_row_lesson_schedule"
        android:textAppearance="@style/ScheduleRoom"
        android:fontFamily="@font/roboto_light"
        android:layout_alignLeft="@+id/textview_card_buildingnumber_row_lesson_schedule"
        android:layout_below="@+id/textview_card_buildingnumber_row_lesson_schedule"
        android:layout_marginTop="@dimen/margintop_card_row_lesson_schedule_inner"/>
        <TextView
        android:text="@string/group_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_group_row_lesson_schedule"
        android:textAppearance="@style/ScheduleRoom"
        android:fontFamily="@font/roboto_light"
        android:layout_alignLeft="@+id/textview_card_buildingnumber_row_lesson_schedule"
        android:layout_below="@+id/textview_card_teacher_row_lesson_schedule"
        android:layout_marginTop="@dimen/margintop_card_row_lesson_schedule_inner"
        android:layout_marginBottom="@dimen/marginbottom_card_row_lesson_schedule" />
	
	
	<TextView
        android:text="@string/type_schedule_row"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_card_type_row_lesson_schedule"
        android:textAppearance="@style/ScheduleRoom"
        android:fontFamily="@font/roboto_light"
        android:layout_alignParentRight="true"
        android:layout_alignBaseline="@+id/textview_card_subject_row_lesson_schedule"
        android:layout_marginRight="@dimen/marginlr_card_row_lesson_schedule_outer" />
</RelativeLayout>